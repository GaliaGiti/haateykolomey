        <%= error %>
      </div>
<% } %>



<form action="/users/login" method="POST">
<div class="login-page">
    <div class="form">
        
        <form class="login-form">
            <div class="form-group">
            <input type="email" id="email" name="email" class="form-control" placeholder="email id">
            </div>
            <div class="form-group">
            <input type="password" id="password" name="password" class="form-control" placeholder="password">
           </div>
            <!-- <input id="login" type = "button" onclick="window.location.href = './chat.html'" value ="Log in"/>-->
           <button type = "submit">Log in</button>
            <p class="message">Not Registered? <a href="/users/register">Register</a></p>
            
        </form>
    </div>
    
</div>
</form>
